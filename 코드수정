import com.tov_banah.sampleroad.Activity.Onboarding.OnboardingActivity // 온보딩 액티비티 임포트

35번째 줄 // 온보딩 페이지를 항상 호출 showOnboarding() } 
private fun showOnboarding() { val intent = Intent(this, OnboardingActivity::class.java) startActivity(intent) } fun getFcmToken(): String { return PreferenceManager.getDefaultSharedPreferences(applicationContext).getString("FcmToken", null) ?: "" }
